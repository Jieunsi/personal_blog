{
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@/*": ["./*"],
      "@core/*": ["./core/*"],
      "@controllers/*": ["./app/controllers/*"],
      "@dao/*": ["./app/dao/*"],
      "@models/*": ["./app/models/*"],
      "@api/*": ["./app/api/v1/*"],
      "@middlewares/*": ["./middlewares/*"],
      "@service/*": ["./app/service/*"],
      "@lib/*": ["./app/lib/*"],
      "@validators/*": ["./app/validators/*"]
    }
  }
}